<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Neronix17.OuterRim.RebelAlliance</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[@Name="MercenaryGunnerBase"]/weaponTags</xpath>
					<value>
						<li>ORRebelStandard</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[@Name="MercenaryHeavyBase"]/weaponTags</xpath>
					<value>
						<li>ORRebelHeavyExplosive</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/PawnKindDef[@Name="MercenaryEliteTierBase"]/weaponTags</xpath>
					<value>
						<li>ORRebelStandard</li>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>	
</Patch>